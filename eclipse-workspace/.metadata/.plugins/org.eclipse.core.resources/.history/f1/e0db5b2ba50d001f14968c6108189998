package superCallingStatement;

class GranndParents{
	GranndParents(){
	System.out.println("Hi");	
	}
}
class Parents extends GranndParents{
	Parents(int a,int b)
	{
		
		System.out.println(a+b);
	}
}
public class Explicitly_Super_Calling extends Parents{
	Explicitly_Super_Calling(){
		super(2,3);
		System.out.println("Hello");
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub\
		Explicitly_Super_Calling S1 = new Explicitly_Super_Calling();

	}

}
